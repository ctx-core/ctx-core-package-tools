{
  "name": "@ctx-core/ctx-core-package-tools",
  "version": "6.4.0",
  "description": "package tools for the @ctx-core monorepo",
  "keywords": [
    "ctx-core"
  ],
  "homepage": "https://github.com/ctx-core/ctx-core-package-tools#readme",
  "bugs": {
    "url": "https://github.com/ctx-core/ctx-core-package-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ctx-core/ctx-core-package-tools/issues"
  },
  "license": "Apache-2.0",
  "author": "Brian Takita",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "package-refactor": "bin/package-refactor.js"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -f tsconfig.tsbuildinfo",
    "compile": "./node_modules/.bin/tsc -b .",
    "exec": "$@"
  },
  "dependencies": {
    "@ctx-core/array": "workspace:^20.5.0"
  },
  "devDependencies": {
    "@types/glob": "~7.1.3",
    "@types/node": "^15.6.1",
    "esm": "^3.2.25",
    "globby": "^11.0.3",
    "typescript": "^4.2.4"
  },
  "publishConfig": {
    "access": "public",
    "cache": "~/.npm"
  }
}
