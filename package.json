{
	"name": "@ctx-core/ctx-core-package-tools",
	"version": "8.2.17",
	"description": "package tools for the @ctx-core monorepo",
	"keywords": [
		"ctx-core"
	],
	"homepage": "https://github.com/ctx-core/ctx-core-package-tools#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/ctx-core-package-tools/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ctx-core-package-tools/issues"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-package-tools@briantakita.me"
	},
	"type": "module",
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"bin",
		"package_refactor"
	],
	"bin": {
		"package-refactor": "bin/package-refactor.mjs",
		"symlink_src_assets_to_dist": "bin/symlink_src_assets_to_dist.sh"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@"
	},
	"dependencies": {
		"dir-glob": "^3.0.1",
		"path-type": "^5.0.0"
	},
	"devDependencies": {
		"@types/node": "^20.14.5"
	},
	"publishConfig": {
		"access": "public"
	}
}